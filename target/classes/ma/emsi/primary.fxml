<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox id="login_bakcground" alignment="CENTER" prefHeight="297.0" prefWidth="555.0" spacing="20.0" stylesheets="@css/login.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ma.emsi.Controllers.PrimaryController">
   <children>
      <Label id="login_label" text="LOGIN" />
      <TextField fx:id="usernameTextField" promptText="Username" style="-fx-max-width: 250;" />
      <PasswordField fx:id="passwordPasswordField" promptText="Password" style="-fx-max-width: 250;" />
      <Button fx:id="loginButton" onAction="#switchToSecondary" text="Login" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
